    "requires": {
      "auth": "mocked",
      "connectivity": true,
      "destinations": true,
      "html5-repo": true,
      "application-logging": true,
      "CustomDutyInvoiceSrv": {
        "kind": "odata",
        "model": "srv/external/CustomDutyInvoiceSrv",
        "[production]": {
          "credentials": {
            "destination": "STIHL_B2B_K4Q100_BASIC",
            "path": "/sap/opu/odata4/sap/zapi_mm_customsinvdetails_v4/srvd_a2x/sap/zapi_mm_customdutyinvdetails/0001"
          }
        },
        "[hybrid]": {
          "credentials": {
            "destination": "STIHL_B2B_K4Q100_BASIC",
            "path": "/sap/opu/odata4/sap/zapi_mm_customsinvdetails_v4/srvd_a2x/sap/zapi_mm_customdutyinvdetails/0001"
          }
        },
        "csrf": true,
        "csrfInBatch": true
      },      
      "API_SUPPLIERINVOICE_PROCESS_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/API_SUPPLIERINVOICE_PROCESS_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "[production]": {
          "credentials": {
            "destination": "STIHL_B2B_K4Q100_BASIC",
            "path": "/sap/opu/odata/sap/API_SUPPLIERINVOICE_PROCESS_SRV"
          }
        },
        "[hybrid]": {
          "credentials": {
            "destination": "STIHL_B2B_K4Q100_BASIC",
            "path": "/sap/opu/odata/sap/API_SUPPLIERINVOICE_PROCESS_SRV"
          }
        }        
      },
      "[production]": {
        "auth": "xsuaa"
      }
    }
